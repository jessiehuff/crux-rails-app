<%= form_for @project do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %> <br><br>
  </div>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %> <br><br>
  </div>

  <div class="checkboxes">
    <%= f.collection_check_boxes :tag_ids, Tag.all, :id, :name %>
  </div>

  <div class="field">
    <p>Create a New Tag:</p>
    <%= f.label :tag_name %>
    <%= f.text_field :tag_name %> <br><br>
  </div> #something's wrong here

  <div class="action">
    <%= f.submit %>
  </div>
<% end %>
